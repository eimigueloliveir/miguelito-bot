@model Guild
@using DSharpPlus.Entities;
<style>
    * {
        margin: 0;
        padding: 0;
        outline: 0;
        box-sizing: border-box;
        border: 0;
    }
</style>

<link rel="stylesheet" href="~/css/dashboard/sidebar.css">

<div class="col-md-1 bg-dark bar sidebar" id="sidebar">
   
     
    @{
        DiscordUser User = await miguelito_bot_site.Utils.Auth2.User(Context.Request.Cookies["token"]);
        <a href="/dashboard"><img class="server" src="@User.AvatarUrl" alt="Home"></a>
        <div class="barinha"></div>
        foreach(DiscordGuild guild in @Model.Guilds)
        {
            string img = guild.IconUrl;
            if(string.IsNullOrEmpty(img)){
                img = "/img/Miguelito.png"; 
            }
           <a href="aaaaaaaaaaaaaaaaaaaaaaaaaa"><img class="server" src="@img" alt""/></a> 
        }
    }


        </div>
    <div class="container">
    <div class="row" id="row-main">
       
        <div class="col-md-9" id="content">
            ...
        </div>
    </div>
</div>
