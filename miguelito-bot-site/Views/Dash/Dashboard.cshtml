@model guild
@using DSharpPlus.Entities;
<style>
    * {
        margin: 0;
        padding: 0;
        outline: 0;
        box-sizing: border-box;
        border: 0;
    }

    .server {
        margin: 5px;
        width: 70px;
        border-radius: 50%;
    }

    .barinha {
        height: 1px;
        width: 100%;
        background-color: rgb(54, 54, 54);
    }

</style>

 <div class="col-md-1 bg-dark bar" id="sidebar" style="text-aling: center;">
   
     
    @{
        DiscordUser User = await miguelito_bot_site.Utils.Auth2.User(Context.Request.Cookies["token"]);
        <a href="aaaaaaaaaaaaaaaaaaaaaaaaaa"><img class="server" src="@User.AvatarUrl" alt="Home"></a>
        <div style="barinha"></div>
        foreach(DiscordGuild guild in @Model.guilds)
        {
            string img = guild.IconUrl;
            if(string.IsNullOrEmpty(img)){
                img = "/img/Miguelito.png"; 
            }
           <a href="aaaaaaaaaaaaaaaaaaaaaaaaaa"><img class="server" src="@img" alt""/></a> 
        }
    }


        </div>
<div class="container">
    <div class="row" id="row-main">
       
